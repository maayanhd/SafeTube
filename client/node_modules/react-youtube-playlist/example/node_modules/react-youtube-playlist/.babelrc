{
  "presets": [ "react", "es2015", "es2017", "stage-2"],
  "plugins": [
    ["transform-react-jsx"],
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "utils": "./utils"
      }
    }],
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["transform-async-to-generator"]
  ]
}
